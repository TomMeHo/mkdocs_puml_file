    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "Install Plugin",
            "name": "Python Debugger: MkDocs",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "mkdocs ",
            "args": ["serve", "-a 0.0.0.0:8002", "-f example/mkdocs.yml"]
        },
    ]
}